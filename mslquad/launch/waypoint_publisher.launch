<?xml version="1.0" encoding="UTF-8"?>

<launch>
    <arg name="mav_name" default="quad"/>
    <arg name="id" default="0"/>
    <arg name="file_path" default="$(env HOME)/catkin_ws/src/msl_quad/mslquad/resource/mbc_waypoints_sender_1.txt"/>

    <group ns="$(arg mav_name)$(arg id)">
        <node pkg="mslquad" type="waypoint_publisher" name="waypoint_publisher"
              args="$(arg mav_name)$(arg id) $(arg file_path)" output="screen">
        </node>
    </group>

</launch>
